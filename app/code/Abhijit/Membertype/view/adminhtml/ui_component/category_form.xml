<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general">
       <field name="name">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="required" xsi:type="boolean">true</item>
                    <item name="validation" xsi:type="array">
                        <item name="required-entry" xsi:type="boolean">true</item>
                    </item>
                    <item name="sortOrder" xsi:type="number">10</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" translate="true" xsi:type="string">Membertype Name</item>
                </item>
            </argument>
        </field>
        <field name="membership_category" sortOrder="12" formElement="select">
            <settings>
                <dataType>int</dataType>
                <label translate="true">Membership Category</label>                    
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Abhijit\Membertype\Model\Config\Membershipcategory"/>
                    </settings>
                </select>
            </formElements>
        </field> 
        <field name="chapter_type">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">boolean</item>
                    <item name="formElement" xsi:type="string">checkbox</item>
                    <item name="label" xsi:type="string" translate="true">Chapter Type</item>
                    <item name="prefer" xsi:type="string">toggle</item>
                    <item name="valueMap" xsi:type="array">
                        <item name="true" xsi:type="string">1</item>
                        <item name="false" xsi:type="string">0</item>
                    </item>
                    <item name="default" xsi:type="number">0</item>
                </item>
            </argument>
        </field>
        <field name="invoice_type" sortOrder="40" formElement="select">
            <settings>
                <dataType>string</dataType>
                <label translate="true">Invoice Type</label>                    
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Abhijit\Membertype\Model\Config\Invoicetype"/>
                    </settings>
                </select>
            </formElements>
        </field>
        <field name="percent_paid">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="required" xsi:type="boolean">false</item>
                    <item name="validation" xsi:type="array">
                        <item name="required-entry" xsi:type="boolean">false</item>
                    </item>
                    <item name="sortOrder" xsi:type="number">333</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" translate="true" xsi:type="string">Percent Paid</item>
                </item>
            </argument>
        </field>
        <field name="membertype_status" sortOrder="40" formElement="select">
            <settings>
                <dataType>string</dataType>
                <label translate="true">Membertype Status</label>                    
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Abhijit\Membertype\Model\Config\Membertypestatus"/>
                    </settings>
                </select>
            </formElements>
        </field> 
        <field name="customer_type" sortOrder="40" formElement="select">
            <settings>
                <dataType>string</dataType>
                <label translate="true">Customer Type</label>                    
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Abhijit\Membertype\Model\Config\Customertype"/>
                    </settings>
                </select>
            </formElements>
        </field>  
        <field name="exclude_from_yos">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">boolean</item>
                    <item name="formElement" xsi:type="string">checkbox</item>
                    <item name="label" xsi:type="string" translate="true">Exclude from YOS</item>
                    <item name="prefer" xsi:type="string">toggle</item>
                    <item name="valueMap" xsi:type="array">
                        <item name="true" xsi:type="string">1</item>
                        <item name="false" xsi:type="string">0</item>
                    </item>
                    <item name="default" xsi:type="number">0</item>
                </item>
            </argument>
        </field>
        <field name="receive_member_benefits">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">boolean</item>
                    <item name="formElement" xsi:type="string">checkbox</item>
                    <item name="label" xsi:type="string" translate="true">Receive Member Benefits</item>
                    <item name="prefer" xsi:type="string">toggle</item>
                    <item name="valueMap" xsi:type="array">
                        <item name="true" xsi:type="string">1</item>
                        <item name="false" xsi:type="string">0</item>
                    </item>
                    <item name="default" xsi:type="number">0</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>